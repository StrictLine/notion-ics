{
	"type": "module",
	"name": "notion-ics",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"start": "npm run build && npm run preview",
		"dev": "vite dev --debug --host",
		"build": "vite build",
		"preview": "vite preview --strictPort --port 5173 --host 0.0.0.0",
		"link": "NODE_NO_WARNINGS=1 node --loader ts-node/esm ./src/generate-link.ts",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"cross-env": "^7.0.3",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^3.1.1",
		"svelte": "^5.0.0",
		"svelte-check": "^3.6.6",
		"svelte-eslint-parser": "^1.3.3",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"@notionhq/client": "^2.3.0",
		"@sveltejs/adapter-node": "^5.3.3",
		"@sveltejs/kit": "^2.43.7",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"cookie": "^1.0.2",
		"dotenv": "^16.3.1",
		"ical-generator": "^4.1.0",
		"moment": "^2.30.1",
		"moment-timezone": "^0.5.45",
		"prompt-sync": "^4.2.0",
		"svelte-preprocess": "^5.0.4",
		"ts-node": "^10.9.2",
		"vite": "^7.1.9"
	}
}
