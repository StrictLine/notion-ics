{
	"configurations": [
		{
			"name": "Attach by Process ID",
			"processId": "${command:PickProcess}",
			"request": "attach",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"name": "Launch run dev",
			"request": "launch",
			"runtimeArgs": [
				"run",
				"dev"
			],
			"runtimeExecutable": "npm",
			// "smartStep": true,
			"skipFiles": [
				"<node_internals>/**",
			],
			"type": "node",
		},
		
		{
			"name": "Docker Node.js Launch",
			"type": "docker",
			"request": "launch",
			"preLaunchTask": "docker-run: debug",
			"platform": "node"
		}
	]
}